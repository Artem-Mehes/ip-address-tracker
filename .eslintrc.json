{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", "tab"],
        "semi": ["error", "always"],
        "no-console": ["error"],
        "no-undef": ["off"],
        "no-tabs": ["off"],
        "comma-dangle": ["off"]
    }
}
