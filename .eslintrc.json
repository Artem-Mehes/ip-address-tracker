{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "semi": ["error", "always"],
        "no-console": ["error"],
        "no-undef": ["off"]
    }
}
